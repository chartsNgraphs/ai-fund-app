datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:PostgresAdmin@localhost:5432/SECAGENT?schema=app"
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}